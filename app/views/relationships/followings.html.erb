<div class="container">
  <h2>follow</h2>
  <div class="row">
    <div class="col-md-6">
      <table>
       <% @users.each do |user| %>
        <tr>
          <td class="px-3 py-3">
          <%=image_tag user.get_profile_image(50,50),class: "rounded-circle" %>
          </td>
          <td class="py-3">
            <%=link_to user.name,user_path(user) %>
          </td>
        </tr>
       <% end %>
      </table>
    </div>
  </div>
</div>