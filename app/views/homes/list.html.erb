<div class="container">
  <h1>List</h1>
  <div class="row">
    <div class="col-5">
      <% if user_signed_in? %>
      <%=form_with url: search_path,method: :get do |f| %>
      <%=f.text_field :word %>
      <%=f.select :model,options_for_select({"User"=>"user","Post"=>"post"})%>
      <%=f.select :search,options_for_select({"完全一致"=>"perfect","前方一致"=>"forward","後方一致"=>"backword","部分一致"=>"partial"})%>
      <%=f.submit "検索" %>
      <% end %>
      <% end %>
      <p>ゾウ</p>
      <p>くま</p>
      <p>ネコ</p>
      <p>キリン</p>
      <p>イヌ</p>
    </div>
    <div class="col-7">
      <h2>welcome</h2>
      <p>像の上手な作りかた</p>
    </div>
  </div>
</div>